extends ../layouts/main-layout.pug
include ../mixins/createSlide.pug
include ../mixins/services.pug
include ../mixins/productcard.pug
include ../mixins/client.pug
include ../mixins/category.pug
include ../mixins/project.pug
include ../mixins/aboutservices.pug


include ../mixins/shortservices.pug

block styles
  script(type='text/javascript').
    link('/css/otherproduct.min.css')
    link('/css/project.css')

block content
    #carouselExampleIndicators.w-100.h-auto
      -var index=0;
      -var load="data:image/gif;base64,";
      each slide in posts
        if slide.type=="slide"
          .carousel-inner
            .ct-header.tablex.item
                
                img#slide.animated.w-100.h-25(src=`${load}` data-lazy=`/img/resized/${slide.slide.image}` alt=`${slide.slide.title}`)
                -index++;
                span.carousel-caption
                  if slide.slide.title!=""
                      h4.animated(style='animation-delay: 1s;' class=`${slide.slide.animate}`) #{slide.slide.title}
                  if slide.slide.description!=""
                  
                      p.h6.animated(style='animation-delay: 1s; font-size: 1rem;' class=`${slide.slide.animate}`)
                          | #{slide.slide.description}
                  if slide.slide.buttonName!=""
                      p.h6.animated(style='animation-delay: 1s' class=`${slide.slide.animate}`)
                        a(href=`${slide.slide.buttonLink.toString().split(",")[0]}`)
                          button.btn.btn-warning(style="font-size: 1rem;") #{slide.slide.buttonName}
                if index==1
                  script.
                      $("#slide").first().attr("src",`data:image/gif;base64,R0lGODlhAgABAIAAAP///wAAACH5BAAAAAAALAAAAAACAAEAAAICBAoAOw==`)

    .container
      h2.text-center.mt-5
        strong ÜRÜNLERİMİZ

      .row

        each product in products
          +productcard(product)
        button.btn.btn-warning.col-md-12 
          a(href="/products" style="text-decoration: none;").text-dark  Daha Fazla Ürün Göster
    br
    br
    +category(categories)

    +shortservices(posts)
    br
    +project(posts)
    br
    
    +aboutservices(posts)
    .slide-for
    br
    +client(globalvalue) 
    br
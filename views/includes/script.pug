        url: '/api/logo',

        type: 'GET',

        data: { format: 'json' },

        success: function(data) {

        revizeverim=JSON.stringify(data.logo.logo);
        logo=JSON.parse(revizeverim);
        $("#logo").attr(`src`,`/img/${logo}`);

        navbarLight=JSON.stringify(data.themes.navbarLight);
        navlightcolor=JSON.parse(navbarLight);
        navbarDark=JSON.stringify(data.themes.navbarDark);
        navdarkcolor=JSON.parse(navbarDark);
        

        $(':root').css('--navbarLight',navlightcolor,)
        $(':root').css('--navbarDark',navdarkcolor)

        infoLight=JSON.stringify(data.themes.infoLight);
        infolightcolor=JSON.parse(infoLight);
        infoDark=JSON.stringify(data.themes.infoDark);
        infodarkcolor=JSON.parse(infoDark);

        $(':root').css('--infoLight',infolightcolor)
        $(':root').css('--infoDark',infodarkcolor)

        cardLight=JSON.stringify(data.themes.cardLight);
        cardlightcolor=JSON.parse(cardLight);
        cardDark=JSON.stringify(data.themes.cardrDark);
        carddarkcolor=JSON.parse(cardDark);
        
        $(':root').css('--cardLight',cardlightcolor)
        $(':root').css('--cardDark',carddarkcolor)

        bodyLight=JSON.stringify(data.themes.bodyLight);
        bodylightcolor=JSON.parse(bodyLight);
        bodyDark=JSON.stringify(data.themes.bodyDark);
        bodydarkcolor=JSON.parse(bodyDark);

        $(':root').css('--bodyLight',bodylightcolor)
        $(':root').css('--bodyDark',bodydarkcolor)
        

        footerLight=JSON.stringify(data.themes.footerLight);
        footerlightcolor=JSON.parse(footerLight);
        footerDark=JSON.stringify(data.themes.footerDark);
        footerdarkcolor=JSON.parse(footerDark);        

        $(':root').css('--footerLight',footerlightcolor)
        $(':root').css('--footerDark',footerdarkcolor)

    

        },
        });

    });

